
@font-face {
  font-family: 'icomoon';
  src:  url('./icomoon.eot?hacsu');
  src:  url('./icomoon.eot?hacsu#iefix') format('embedded-opentype'),
          url('./icomoon.ttf?hacsu') format('truetype'),
          url('./icomoon.woff?hacsu') format('woff'),
          url('./icomoon.svg?hacsu#icomoon') format('svg');
  font-weight: normal;
  font-style: normal;
  font-display: block;
}

[class^="icon-"], [class*=" icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'icomoon' !important;
  speak: never;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-google-plus:before {
  content: "\ea8b";
}
.icon-facebook:before {
  content: "\ea90";
}
.icon-twitter:before {
  content: "\ea96";
}