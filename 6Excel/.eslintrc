{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "indent": ["error", 4],
    "linebreak-style": ["error", "windows"],
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "operator-linebreak": ["error", "before"]
  },
  "extends": [
    "eslint:recommended",
    "google"
  ]
}